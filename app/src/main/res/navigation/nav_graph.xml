<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/registerFragment">
    <fragment
        android:id="@+id/registerFragment"
        android:name="kh.edu.rupp.ite.memo.ui.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registerFragment_to_onboading_1_Fragment"
            app:destination="@id/onboading_1_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="kh.edu.rupp.ite.memo.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_onboading_1_Fragment"
            app:destination="@id/onboading_1_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="kh.edu.rupp.ite.memo.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_noteFragment"
            app:destination="@id/noteFragment" />
    </fragment>
    <fragment
        android:id="@+id/noteFragment"
        android:name="kh.edu.rupp.ite.memo.ui.note.NoteFragment"
        android:label="fragment_note"
        tools:layout="@layout/fragment_note" >
        <action
            android:id="@+id/action_noteFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/onboading_1_Fragment"
        android:name="kh.edu.rupp.ite.memo.ui.loading.Onboading_2_Fragment"
        android:label="fragment_onboading_1_"
        tools:layout="@layout/fragment_onboading_1_" >
        <action
            android:id="@+id/action_onboading_1_Fragment_to_onboading_2_Fragment2"
            app:destination="@id/onboading_2_Fragment2" />
    </fragment>
    <fragment
        android:id="@+id/onboading_3_Fragment"
        android:name="kh.edu.rupp.ite.memo.ui.loading.Onboading_3_Fragment"
        android:label="fragment_onboading_3_"
        tools:layout="@layout/fragment_onboading_3_" >
        <action
            android:id="@+id/action_onboading_3_Fragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/onboading_2_Fragment2"
        android:name="kh.edu.rupp.ite.memo.ui.loading.Onboading_2_Fragment"
        android:label="fragment_onboading_2_"
        tools:layout="@layout/fragment_onboading_2_" >
        <action
            android:id="@+id/action_onboading_2_Fragment2_to_onboading_3_Fragment"
            app:destination="@id/onboading_3_Fragment" />
    </fragment>
</navigation>